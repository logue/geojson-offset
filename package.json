{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "geojson-offset-ts",
  "version": "0.1.4",
  "license": "MIT",
  "description": "Add offset to the coordinates of GeoJSON.",
  "keywords": [
    "geojson",
    "feature",
    "geometry",
    "coordinate",
    "offset"
  ],
  "author": {
    "name": "Haoliang Yu",
    "email": "haholiang.yu@outlook.com"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "homepage": "https://github.com/logue/geojson-offset",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/geojson-offset.git"
  },
  "bugs": {
    "url": "https://github.com/logue/geojson-offset/issues"
  },
  "main": "dist/geojson-offset.umd.js",
  "module": "dist/geojson-offset.es.js",
  "types": "dist/geojson-offset.d.ts",
  "exports": {
    ".": {
      "import": "./dist/geojson-offset.es.js",
      "require": "./dist/geojson-offset.umd.js",
      "types": "./dist/geojson-offset.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.3.0",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf node_modules/.vite",
    "type-check": "tsc -p tsconfig.vitest.json --noEmit --composite false",
    "build": "run-p type-check build-only build-declaration",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "build-declaration": "tsc -p tsconfig.app.json --composite false --declaration --declarationMap --declarationDir dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "test:unit": "vitest --root src/",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.10",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vite-plugin-checker": "^0.5.2",
    "vitest": "^0.25.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.8.1"
  }
}
