{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "geojson-offset-ts",
  "version": "0.1.3",
  "description": "Add offset to the coordinates of GeoJSON.",
  "files": [
    "dist"
  ],
  "main": "./dist/geojson-offset.umd.js",
  "module": "./dist/geojson-offset.es.js",
  "exports": {
    ".": {
      "import": "./dist/geojson-offset.es.js",
      "require": "./dist/geojson-offset.umd.js"
    }
  },
  "types": "./dist/geojson-offset.d.ts",
  "keywords": [
    "geojson",
    "feature",
    "geometry",
    "coordinate",
    "offset"
  ],
  "author": {
    "name": "Haoliang Yu",
    "email": "haholiang.yu@outlook.com"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "homepage": "https://github.com/logue/geojson-offset",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/geojson-offset.git"
  },
  "bugs": {
    "url": "https://github.com/logue/geojson-offset/issues"
  },
  "engines": {
    "node": ">=16.15.0",
    "yarn": ">=1.22.4"
  },
  "packageManager": "yarn@3.2.1",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf node_modules/.vite",
    "build": "vite build && tsc",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "test": "vite --config vite.config.test.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/geojson": "^7946.0.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "puppeteer": "^14.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-test": "^0.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  }
}
