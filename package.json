{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "geojson-offset-ts",
  "version": "0.1.1",
  "description": "Add offset to the coordinates of GeoJSON.",
  "files": [
    "dist"
  ],
  "main": "./dist/geojson-offset.umd.js",
  "module": "./dist/geojson-offset.es.js",
  "exports": {
    ".": {
      "import": "./dist/geojson-offset.es.js",
      "require": "./dist/geojson-offset.umd.js"
    }
  },
  "types": "./dist/geojson-offset.d.ts",
  "keywords": [
    "geojson",
    "feature",
    "geometry",
    "coordinate",
    "offset"
  ],
  "author": {
    "name": "Haoliang Yu",
    "email": "haholiang.yu@outlook.com"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "homepage": "https://github.com/logue/geojson-offset",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/geojson-offset.git"
  },
  "bugs": {
    "url": "https://github.com/logue/geojson-offset/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "vite build && tsc",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "test": "vite --config vite.config.test.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@modyqyw/vite-plugin-eslint": "^1.1.0",
    "@types/chai": "^4.3.0",
    "@types/geojson": "^7946.0.8",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "chai": "^4.3.6",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "prettier-plugin-jsdoc": "^0.3.33",
    "prettier-plugin-md-nocjsp": "^1.2.0",
    "puppeteer": "^13.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vite-plugin-test": "^0.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  }
}
